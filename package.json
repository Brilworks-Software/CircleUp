{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "npx expo prebuild --clean",
    "web:export": "npx expo export -p web",
    "web:deploy": "npx eas deploy",
    "build:android:develop": "npx eas build --platform android --profile develop --non-interactive",
    "build:ios:develop": "npx eas build --platform ios --profile develop --non-interactive",
    "build:ios:production:local": "npx eas build --platform ios --profile production --local",
    "build:android:production": "npx eas build --platform android --profile production --non-interactive",
    "build:ios:production": "eas build --platform ios --profile production --non-interactive",
    "submit:android:production": "npx eas submit --platform android --profile production --non-interactive",
    "submit:ios:production": "npx eas submit --platform ios --profile production --latest --non-interactive",
    "update:ios:production": "eas update --channel production"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-firebase/analytics": "latest",
    "@react-native-firebase/app": "latest",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/react-query": "^5.90.2",
    "expo": "~54.0.27",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.11",
    "expo-contacts": "~15.0.11",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-notifications": "~0.32.14",
    "expo-router": "~6.0.17",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.3.0",
    "lucide-react-native": "^0.475.0",
    "mobx": "^6.13.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.2.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "dotenv": "^17.2.2",
