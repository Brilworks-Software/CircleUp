{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "npx expo prebuild --clean",
    "web:export": "npx expo export -p web",
    "web:deploy": "npx eas deploy",
    "build:android:develop": "npx eas build --platform android --profile develop --non-interactive",
    "build:ios:develop": "npx eas build --platform ios --profile develop --non-interactive",
    "build:ios:production:local": "npx eas build --platform ios --profile production --local",
    "build:android:production": "npx eas build --platform android --profile production --non-interactive",
    "build:ios:production": "eas build --platform ios --profile production --non-interactive",
    "submit:android:production": "npx eas submit --platform android --profile production --non-interactive",
    "submit:ios:production": "npx eas submit --platform ios --profile production --non-interactive",
    "update:ios:production": "eas update --channel production"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/react-query": "^5.90.2",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.3",
    "expo-constants": "~17.1.3",
    "expo-contacts": "^15.0.8",
    "expo-font": "~13.2.2",
    "expo-haptics": "~14.1.3",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "^8.0.8",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.0.2",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-system-ui": "~5.0.5",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.1.5",
    "firebase": "^12.3.0",
    "lucide-react-native": "^0.475.0",
    "mobx": "^6.13.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "dotenv": "^17.2.2",
    "typescript": "~5.8.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
