{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "develop": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "channel": "develop",
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "bhavik.b@brilworks.com",
        "ascAppId": "6753784281", 
        "appleTeamId": "K7Y5QY6LMX", 
        "bundleIdentifier": "com.brilworks.circleUp"
      }
    }
  }
}
